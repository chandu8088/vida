<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Charging locator choose plan V2"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <chargingLocator
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Charing Locator config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <titleOne
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Main heading subtext"
                                name="./titleOne" />
                            <titleTwo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Main heading"
                                name="./titleTwo" />
                            <customtitletagtwo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                fieldDescription="Please Select the heading tag for title"
                                emptyText="Select heading tag"
                                fieldLabel="Heading Tag"
                                name="./customTitleTwoTag">
                                <items jcr:primaryType="nt:unstructured">
                                    <default
                                        jcr:primaryType="nt:unstructured"
                                        text="Select Tag"
                                        value=""/>
                                    <h1
                                        jcr:primaryType="nt:unstructured"
                                        text="h1"
                                        value="h1"/>
                                    <h2
                                        jcr:primaryType="nt:unstructured"
                                        text="h2"
                                        value="h2"/>
                                    <h3
                                        jcr:primaryType="nt:unstructured"
                                        text="h3"
                                        value="h3"/>
                                    <h4
                                        jcr:primaryType="nt:unstructured"
                                        text="h4"
                                        value="h4"/>
                                    <h5
                                        jcr:primaryType="nt:unstructured"
                                        text="h5"
                                        value="h5"/>
                                    <h6
                                        jcr:primaryType="nt:unstructured"
                                        text="h6"
                                        value="h6"/>
                                </items>
                            </customtitletagtwo>
                            <headerContent
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Main heading sub content"
                                name="./headerContent" />
                            <chargeLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Charging label text"
                                name="./chargeLabel" />
                            <chargeContent
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Charge content text"
                                name="./chargeContent" />
                            <pluginImgMobile
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Plugin Icon Mobile"
                                name="./pluginImgMobile"
                                rootPath="/content/dam" />
                            <pluginImgDesktop
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Plugin Icon desktop"
                                name="./pluginImgDesktop"
                                rootPath="/content/dam" />
                            <btnLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Get my plan Button label"
                                name="./btnLabel" />
                            <navLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Getmyplan Button navigation Url"
                                name="./navLink"
                                rootPath="/content" />
                            <newTab
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                name="./newTab"
                                text="Open link in new tab"
                                fieldDescription="If checked, the link will be opened in a new browser tab."
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true" />
                            <scrollImgMobile
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Scrolling image mobile"
                                name="./scrollImgMobile"
                                rootPath="/content/dam" />
                            <scrollImgDesktop
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Scrolling image desktop"
                                name="./scrollImgDesktop"
                                rootPath="/content/dam" />
                            <dropDownTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Dropdown title"
                                name="./dropDownTitle" />
                            <dropDownArrowIcon
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Dropdown arrow Icon"
                                name="./dropDownArrowIcon"
                                rootPath="/content/dam" />
                            <planContentheader
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Plan content tabs header"
                                name="./planContentheader" />
                            <dropDownContent
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldDescription="Dropdown content"
                                fieldLabel="Dropdown content"
                                name="./dropDownContent"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="/apps/vida/components/vida-2.0/structure/footer/cq:dialog/content/items/tabs/items/footerMain/items/columns/items/column/items/footerMainItem/items/footerAddress/rtePlugins" />
                                <uiSettings
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="/apps/vida/components/vida-2.0/structure/footer/cq:dialog/content/items/tabs/items/footerMain/items/columns/items/column/items/footerMainItem/items/footerAddress/uiSettings" />

                                <htmlRules jcr:primaryType="nt:unstructured">
                                    <blockHandling
                                        jcr:primaryType="nt:unstructured"
                                        removeSingleParagraphContainer="{Boolean}true" />
                                </htmlRules>
                            </dropDownContent>
                            <item_details
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldDescription="Click 'Add' to add tab items"
                                fieldLabel="Add Items"
                                validation="multifield-item-number">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./items">
                                    <items
                                        jcr:primaryType="nt:unstructured">
                                        <planType
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Plan type"
                                            name="./planType" />
                                        <price
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Plan price"
                                            name="./price" />
                                        <strikePrice
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                            name="./strikePrice"
                                            text="If checked, the price will be striked"
                                            fieldDescription="If checked, the price will be striked."
                                            uncheckedValue="{Boolean}false"
                                            value="{Boolean}true" />
                                        <freeLabel
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Free label"
                                            name="./freeLabel" />
                                        <tabDescription
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldLabel="Tab Description"
                                            name="./tabDescription" />
                                    </items>
                                </field>
                            </item_details>
                        </items>
                    </chargingLocator>
                    <chargingLocatorCard
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Charing Locator Card config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <cardMainBoldText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Card main bold text"
                                name="./cardMainBoldText" />
                            <cardSubText
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Card sub text"
                                name="./cardSubText" />
                            <cardIcon
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Association Icon"
                                name="./cardIcon"
                                rootPath="/content/dam" />
                            <cardButtonLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Card button label text"
                                name="./cardButtonLabel" />
                            <cardButtonLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Card button link"
                                name="./cardButtonLink"
                                rootPath="/content" />
                        </items>
                    </chargingLocatorCard>
                    <chargingLocatorPlanCard
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Popup config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <myPlanTitle
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                fieldLabel="Popup message"
                                fieldDescription="Please provide popup message for the card"
                                name="./myPlanTitle"
                                useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="/apps/vida/components/richtext/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text/rtePlugins" />
                                <uiSettings
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceSuperType="/apps/vida/components/richtext/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text/uiSettings" />
                                <htmlRules jcr:primaryType="nt:unstructured">
                                    <blockHandling
                                        jcr:primaryType="nt:unstructured"
                                        removeSingleParagraphContainer="{Boolean}true" />
                                </htmlRules>
                            </myPlanTitle>
                            <confirmLink
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                fieldLabel="Confirm button navigation Url"
                                name="./confirmLink"
                                rootPath="/content" />
                            <confirmButton
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Confirm button label text"
                                name="./confirmButton" />
                            <cancelCtaLabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Cancel button label text"
                                name="./cancelCtaLabel" />
                        </items>
                    </chargingLocatorPlanCard>
                    <dataLayer
                        jcr:primaryType="nt:unstructured"
                        jcr:title="DataLayer"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <dataPosition
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="This field specifies the value of linkPosition in the data layer."
                                                fieldLabel="Position"
                                                name="./dataPosition"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </dataLayer>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>