<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="GENERAL"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <sectionid
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Section ID"
                                                name="./sectionid"/>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title"
                                                name="./title"
                                                value="LETS STAY IN TOUCH!"/>
                                            <congratsTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Congratulation Title"
                                                name="./congratsTitle"
                                                value="CONGRATULATIONS!"/>
                                            <message
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Message"
                                                name="./message"
                                                value="Get updates on sales specials and more"/>
                                            <congratsMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Congratulation Message"
                                                name="./congratsMessage"
                                                value="You have been added to our mailing list, you will be the first to know about our new releases"/>
                                            <duplicateMobileNumberMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Duplicate Mobile Number Message"
                                                name="./duplicateMobileNumberMessage"/>
                                            <instaurl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Instagram Link"
                                                name="./instaurl"
                                                rootPath="/content/vida"/>
                                            <twitterurl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Twitter Link"
                                                name="./twitterurl"
                                                rootPath="/content/vida"/>
                                            <linkedinurl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="LinkedIn Link"
                                                name="./linkedinurl"
                                                rootPath="/content/vida"/>
                                            <facebookurl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Facebook Link"
                                                name="./facebookurl"
                                                rootPath="/content/vida"/>
                                            <productImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Product Image"
                                                name="./productImage"
                                                rootPath="/content/dam/vida"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </general>
                    <personal
                        jcr:primaryType="nt:unstructured"
                        jcr:title="PERSONAL"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <firstname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="First Name Label"
                                                name="./firstname"
                                                value="First Name"/>
                                            <fnplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="First Name Placeholder"
                                                name="./fnplchld"
                                                value="Enter First Name"/>
                                            <lastname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Last Name Label"
                                                name="./lastname"
                                                value="Last Name"/>
                                            <lnplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Last Name Placeholder"
                                                name="./lnplchld"
                                                value="Enter Last Name"/>
                                            <countrycode
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please put the country code based on the country selected. In case of multiple code, keep them coma (,) separated."
                                                fieldLabel="Country code"
                                                name="./countrycode"/>
                                            <phone
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone Number Label"
                                                name="./phone"
                                                value="Mobile Number"/>
                                            <phplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone Number Placeholder"
                                                name="./phplchld"
                                                value="9999999999"/>
                                            <email
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email Label"
                                                name="./email"
                                                value="Email ID"/>
                                            <emailplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email Placeholder"
                                                name="./emailplaceholder"
                                                value="Enter Email"/>
                                            <infoLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Info Label"
                                                name="./infoLabel"
                                                value="Mention active account, this will be used to send important mails"/>
                                            <pincodelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="PIN Code Label"
                                                name="./pincodelabel"
                                                value="PIN Code"/>
                                            <pincodeph
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="PIN Code Placeholder"
                                                name="./pincodeph"
                                                value="Enter PIN Code"/>
                                            <submitBtn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Button Label"
                                                name="./submitBtn"
                                                value="Submit"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </personal>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
