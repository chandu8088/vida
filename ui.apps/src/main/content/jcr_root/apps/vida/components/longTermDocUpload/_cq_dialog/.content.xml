<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Long Term Document Upload Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <fieldlabels
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic Labels"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <longTermDocUploadTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Long Term Document Upload Title"
                                                name="./longTermDocUploadTitle"/>
                                            <longTermDocUploadSubTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Long Term Document Upload SubTitle"
                                                name="./longTermDocUploadSubTitle"/>
                                            <longTermDocUploadbackgroundImg
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Background Image path"
                                                name="./longTermDocUploadbackgroundImg"
                                                rootPath="/content/dam/vida"/>
                                            <longTermDocUploadRedMsg
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Long Term Document Upload Redirect Message"
                                                name="./longTermDocUploadRedMsg"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </fieldlabels>
                    <documentsLabel
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Document Label"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <documentFrontTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Document Front Title"
                                                name="./documentFrontTitle"/>
                                            <documentFrontText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Document Front Text"
                                                name="./documentFrontText"/>
                                            <documentFrontDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Document Front Description"
                                                name="./documentFrontDesc"/>
                                            <documentFrontDisclaimer
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Document Front Disclaimer"
                                                name="./documentFrontDisclaimer"/>
                                           <frontUploadButtonText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Upload Front Button Text"
                                                name="./frontUploadButtonText"/>
                                           <documentBackText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Document Back Text"
                                                name="./documentBackText"/>
                                           <documentBackDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Document Back Description"
                                                name="./documentBackDesc"/>
                                            <documentBackDisclaimer
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Document Back Disclaimer"
                                                name="./documentBackDisclaimer"/>
                                            <backUploadButtonText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Document Back Button Text Label"
                                                name="./backUploadButtonText"/>
                                            <primarySubmit
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Primary Submit Label"
                                                name="./primarySubmit"/>
                                             <troubleShootText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Troubleshoot Text Label"
                                                name="./troubleShootText"/>
                                            <troubleShootAction
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Troubleshoot Action Label"
                                                name="./troubleShootAction"/>
                                              <emergencyNumberLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Emergency Number Label"
                                                name="./emergencyNumberLabel"/>
                                             <mobileph
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mobile Number Place Holder"
                                                name="./mobileph"
                                                value="Enter Mobile Number"/>
                                            <relationFieldLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="RelationField Label"
                                                name="./relationFieldLabel"/>
                                           <primaryBtn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Primary Button Label"
                                                name="./primaryBtn"/>
                                      </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </documentsLabel>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
