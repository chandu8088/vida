<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Subscription FAQ V2.0"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <faqInfo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="FAQ Info"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <faq
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <iconFaq
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="Please provide icon for the Frequently asked questions"
                                                        fieldLabel="Icon"
                                                        name="./iconFaq"
                                                        rootPath="/content/dam"/>
                                                    <iconalttext
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Icon Alt Text"
                                                        name="./iconAltText"/>
                                                    <titleFaq
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="Please provide title for the Frequently asked questions"
                                                        fieldLabel="Title"
                                                        name="./titleFaq"/>
                                                    <arrowIconFaq
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="Please provide arrow icon"
                                                        fieldLabel="Arrow Icon"
                                                        name="./arrowIconFaq"
                                                        rootPath="/content/dam"/>
                                                    <arrowIconalttext
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Arrow Icon Alt Text"
                                                        name="./arrowIconAltText"/>
                                                    <contentFaq
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                                        fieldLabel="Content"
                                                        name="./contentFaq"
                                                        useFixedInlineToolbar="{Boolean}true">
                                                        <rtePlugins
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/vida/components/richtext/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text/rtePlugins"/>
                                                        <uiSettings
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceSuperType="/apps/vida/components/richtext/cq:dialog/content/items/tabs/items/properties/items/columns/items/column/items/text/uiSettings"/>
                                                    </contentFaq>
                                                    <bgColor
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Background Color"
                                                        name="./bgColor"/>
                                                    <readmorelabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Read More Label"
                                                        name="./readMoreLabel"/>
                                                    <readmoreredirection
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldDescription="Read More Redirection URL"
                                                        fieldLabel="Read More Redirection URL"
                                                        name="./redirectionUrl"/>
                                                </items>
                                            </faq>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </faqInfo>
                    <dataLayer
                        jcr:primaryType="nt:unstructured"
                        jcr:title="DataLayer"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <dataPosition
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="This field specifies the value of linkPosition in the data layer."
                                                fieldLabel="Position"
                                                name="./dataPosition"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </dataLayer>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
