<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Document Upload"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title"
                                                name="./title"
                                                value="Upload Documents"/>
                                            <primary
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Submit Button Label"
                                                name="./primary"
                                                value="submit"/>
                                            <secondary
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Secondary Button Label"
                                                name="./secondary"
                                                value="I'll do it later"/>
                                            <error
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Error Message"
                                                name="./error"
                                                value="Please upload a valid document"/>
                                            <success
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Success Message"
                                                name="./success"
                                                value="Document uploaded successfully"/>
                                            <fileFormatText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="File Format Text"
                                                name="./fileFormatText"
                                                value="Please upload the files in the format of jpeg, png or pdf"/>
											<finalSubmissionProfile
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Final Submission Profile"
                                                name="./finalSubmissionProfile"
                                                value="Profile"/>
                                            <finalSubmissionSuccess
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Final Submission Success"
                                                name="./finalSubmissionSuccess"
                                                value="Documents uploaded successfully"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </general>
                    <personal
                        jcr:primaryType="nt:unstructured"
                        jcr:title="PERSONAL"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <titlePersonal
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title"
                                                name="./titlePersonal"
                                                value="Passport size picture"/>
                                            <textPersonal
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Button Label"
                                                name="./textPersonal"
                                                value="Upload"/>
                                            <descriptionPersonal
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Description"
                                                name="./descriptionPersonal"
                                                value="Most recent picture advised"/>
                                            <disclaimerPersonal
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="disclaimer"
                                                name="./disclaimerPersonal"
                                                value="Less than 1MB"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </personal>
                    <pancard
                        jcr:primaryType="nt:unstructured"
                        jcr:title="PAN CARD"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <titlepancard
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title"
                                                name="./titlepancard"
                                                value="PAN Card"/>
                                            <textpancard
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Button Label"
                                                name="./textpancard"
                                                value="Upload"/>
                                            <descriptionpancard
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Description"
                                                name="./descriptionpancard"
                                                value="Data should be clearly visible"/>
                                            <disclaimerpancard
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="disclaimer"
                                                name="./disclaimerpancard"
                                                value="Less than 1MB"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </pancard>
                    <insurance
                        jcr:primaryType="nt:unstructured"
                        jcr:title="INSURANCE"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <titleinsurance
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title"
                                                name="./titleinsurance"
                                                value="Insurance Copy"/>
                                            <textinsurance
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Button Label"
                                                name="./textinsurance"
                                                value="Upload"/>
                                            <descriptioninsurance
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Description"
                                                name="./descriptioninsurance"
                                                value="Data should be clearly visible"/>
                                            <disclaimerinsurance
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="disclaimer"
                                                name="./disclaimerinsurance"
                                                value="Less than 1MB"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </insurance>
                    <address
                        jcr:primaryType="nt:unstructured"
                        jcr:title="ADDRESS"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <titleaddress
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title"
                                                name="./titleaddress"
                                                value="Address Proof"/>
                                            <subTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Sub-Title"
                                                name="./subTitle"
                                                value="Billing Address"/>
                                            <disclaimeraddress
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Disclaimer"
                                                name="./disclaimeraddress"
                                                value="Note: Upload document ONLY If the billing address is different from the uploaded aadhar card"/>
                                            <addressHead
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Address Heading"
                                                name="./addressHead"
                                                value="Booking Address"/>
                                            <selectPlaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Select Placeholder"
                                                name="./selectPlaceholder"
                                                value="Select"/>
                                            <uploadLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Upload Label"
                                                name="./uploadLabel"
                                                value="upload"/>
                                            <label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Select Document Label"
                                                name="./selectlabel"
                                                value="Select document to upload"/>
                                            <message
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Address Heading"
                                                name="./message"
                                                value="Document is required."/>
                                            <addresserror
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Address Error Message"
                                                name="./addresserror"
                                                value="Please upload a valid address proof"/>
                                            <addresssuccess
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Address Success Message"
                                                name="./addresssuccess"
                                                value="Address proof uploaded successfully"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </address>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
