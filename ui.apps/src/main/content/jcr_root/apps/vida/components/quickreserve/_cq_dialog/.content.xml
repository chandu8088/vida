<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Quick Reserve"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <reservetitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Reserve Title"
                                                name="./reservetitle"
                                                value="Reserve for just Rs. 2499"/>
                                            <citylabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="City Label"
                                                name="./citylabel"
                                                value="City"/>
                                            <firstname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="First Name Label"
                                                name="./firstname"
                                                value="First Name"/>
                                            <fnplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="First Name Placeholder"
                                                name="./fnplchld"
                                                value="Enter First Name"/>
                                            <lastname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Last Name Label"
                                                name="./lastname"
                                                value="Last Name"/>
                                            <lnplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Last Name Placeholder"
                                                name="./lnplchld"
                                                value="Enter Last Name"/>
                                            <phone
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone Number Label"
                                                name="./phoneregister"
                                                value="Mobile Number"/>
                                            <phplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone Number Placeholder"
                                                name="./phplchld"
                                                value="Enter Mobile Number"/>
                                            <email
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email Label"
                                                name="./email"
                                                value="Email ID"/>
                                            <emailplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email Placeholder"
                                                name="./emailplaceholder"
                                                value="Enter Email"/>
                                            <emailinfolabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email Info Label"
                                                name="./emailinfolabel"
                                                value="Mention active account, this will be used to send important mails"/>											
                                            <quicktestid
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Quick Reserve TnC ID(for experience fragment)"
                                                name="./quicktestid"/>
                                            <iagreelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="I agree Label"
                                                name="./agreelabel"
                                                value="I agree to"/>
                                            <termsAndCondition
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Terms and conditions Label"
                                                name="./termsAndCondition"
                                                value="Terms and Conditions"/>
                                            <agreebtnlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Agree Button Label"
                                                name="./agreebtnlabel"
                                                value="Agree"/>
                                            <closebtnlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Close Button Label"
                                                name="./closebtnlabel"
                                                value="Close"/>
                                            <paymentbtnlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Payment CTA Label"
                                                name="./paymentbtnlabel"
                                                value="Pay Rs. 2499/-"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </general>
                    <scooterinfo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Scooter Info"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <pricetype
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Price Type"
                                                name="./pricetype"/>
                                            <city
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Select City"
                                                name="./city">
                                                <datasource
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="/apps/vida"/>
                                            </city>
                                            <productrange
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Product Range"
                                                name="./productrange"/>
                                            <certifiedrange
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Certified Range"
                                                name="./certifiedrange"/>
                                            <enablecertifiedrange
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                fieldLabel=""
                                                name="./enableCertifiedRange"
                                                text="Enable Certified Range?"
                                                uncheckedValue="false"
                                                value="{Boolean}true"/>
                                            <topspeed
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Top Speed"
                                                name="./topspeed"/>
                                            <acceleration
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Acceleration"
                                                name="./acceleration"/>
                                            <chargingtime
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Charging Time"
                                                name="./chargingtime"/>
                                            <colorsummarylabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Color Summary Label"
                                                name="./colorsummarylabel"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </scooterinfo>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
