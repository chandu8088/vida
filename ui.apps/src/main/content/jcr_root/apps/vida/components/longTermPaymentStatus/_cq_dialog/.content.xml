<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Long Term Payment Status Component"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <fieldlabels
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Basic Labels"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <longTermStatusBackgroundImg
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Long Term Status Background Image"
                                                name="./longTermStatusBackgroundImg"
                                                rootPath="/content/dam/vida"/>
                                            <longTermStatusBackgroundImgMob
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Long Term Status Background Image Mobile"
                                                name="./longTermStatusBackgroundImgMob"
                                                rootPath="/content/dam/vida"/>
                                            <longTermStatusSuccessTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Long Term Status Success Title"
                                                name="./longTermStatusSuccessTitle"/>
                                            <longTermStatusRedirectBtn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Long Term Status Redirect Button"
                                                name="./longTermStatusRedirectBtn"/>
                                            <longTermStatusFailureTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Long Term Status Failure Title"
                                                name="./longTermStatusFailureTitle"/>
                                            <longTermStatusFailRedirectBtn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Long Term Status Failure Redirect Button"
                                                name="./longTermStatusFailRedirectBtn"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </fieldlabels>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
