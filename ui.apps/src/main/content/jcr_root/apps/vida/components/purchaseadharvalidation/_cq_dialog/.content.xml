<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
					<isOptimizedJourneyEnabled
						jcr:primaryType="nt:unstructured"
						sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
						fieldLabel="Enable Optimized Journey?"
						name="./isOptimizedJourneyEnabled"
						text="Enable Optimized Journey?"
						uncheckedValue="false"
						value="{Boolean}true"/>
                    <title
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Title"
                        name="./title"
                        value="Aadhar card authentication"/>
					<description
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Description"
                        name="./description"/>
					<seconddesc
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Sub Text"
                        name="./seconddesc"/>
					<disclaimertext
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Disclaimer Text"
                        name="./disclaimertext"/>
					<othertext
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Other Text"
                        name="./othertext"/>
					<aadharNumberLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Aadhar Number"
                        name="./aadharNumberLabel"
                        value="Aadhar Number"/>
					<aadharNumberph
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Aadhar Number Placeholder"
                        name="./aadharNumberph"
                        value="000000000000"/>
                    <btnLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Verify Button"
                        name="./verifyButton"
                        value="Verify through DigiLocker"/>
                    <manualBtnText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldLabel="Manual Button Text"
                        name="./manualBtnText"
                        value="I will do this manually"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>