<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Product Compare"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <sectionid
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="Fill for in page navigation purpose"
                                        fieldLabel="Section Id"
                                        name="./sectionid"/>
                                    <compareheading
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                        fieldDescription="Please enter title"
                                        fieldLabel="Compare Heading"
                                        name="./compareheading"
                                        useFixedInlineToolbar="{Boolean}true">
                                        <rtePlugins jcr:primaryType="nt:unstructured">
                                            <format
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <paraformat
                                                jcr:primaryType="nt:unstructured"
                                                features="*">
                                                <formats jcr:primaryType="nt:unstructured">
                                                    <default_p
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Paragraph"
                                                        tag="p"/>
                                                    <default_h1
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Heading 1"
                                                        tag="h1"/>
                                                    <default_h2
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Heading 2"
                                                        tag="h2"/>
                                                    <default_h3
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Heading 3"
                                                        tag="h3"/>
                                                    <default_h4
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Heading 4"
                                                        tag="h4"/>
                                                    <default_h5
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Heading 5"
                                                        tag="h5"/>
                                                    <default_h6
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Heading 6"
                                                        tag="h6"/>
                                                    <default_blockquote
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Quote"
                                                        tag="blockquote"/>
                                                    <default_pre
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Preformatted"
                                                        tag="pre"/>
                                                </formats>
                                            </paraformat>
                                            <justify
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <lists
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <links
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <image
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <findreplace
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <subsuperscript
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <spellcheck
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <styles
                                                jcr:primaryType="nt:unstructured"
                                                features="*">
                                                <styles jcr:primaryType="nt:unstructured">
                                                    <orange
                                                        jcr:primaryType="nt:unstructured"
                                                        cssName="txt-color--orange"
                                                        text="Orange"/>
                                                </styles>
                                            </styles>
                                            <table
                                                jcr:primaryType="nt:unstructured"
                                                features="*"/>
                                            <misctools
                                                jcr:primaryType="nt:unstructured"
                                                features="*">
                                                <specialCharsConfig jcr:primaryType="nt:unstructured">
                                                    <chars jcr:primaryType="nt:unstructured">
                                                        <copyright
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#169;"/>
                                                        <trademark
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#8482;"/>
                                                        <registered
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#174;"/>
                                                        <emDash
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#8212;"/>
                                                        <pound
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#163;"/>
                                                        <nbsp
                                                            jcr:primaryType="nt:unstructured"
                                                            entity="&amp;#160;"/>
                                                    </chars>
                                                </specialCharsConfig>
                                            </misctools>
                                        </rtePlugins>
                                        <uiSettings jcr:primaryType="nt:unstructured">
                                            <cui jcr:primaryType="nt:unstructured">
                                                <inline
                                                    jcr:primaryType="nt:unstructured"
                                                    toolbar="[format#bold,format#italic,format#underline,#justify,#lists,subsuperscript#subscript,subsuperscript#superscript,links#modifylink,links#unlink,links#anchor,#image,findreplace#find,findreplace#replace,spellcheck#checktext,misctools#specialchars,misctools#sourceedit,#styles,#paraformat,table#table]">
                                                    <popovers jcr:primaryType="nt:unstructured">
                                                        <justify
                                                            jcr:primaryType="nt:unstructured"
                                                            items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                            ref="justify"/>
                                                        <lists
                                                            jcr:primaryType="nt:unstructured"
                                                            items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                            ref="lists"/>
                                                        <paraformat
                                                            jcr:primaryType="nt:unstructured"
                                                            items="paraformat:getFormats:paraformat-pulldown"
                                                            ref="paraformat"/>
                                                        <styles
                                                            jcr:primaryType="nt:unstructured"
                                                            items="styles:getStyles:styles-pulldown"
                                                            ref="styles"/>
                                                    </popovers>
                                                </inline>
                                            </cui>
                                        </uiSettings>
                                    </compareheading>
                                    <tagline
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Tag Line"
                                        name="./tagline"/>
                                    <pcselectedcity
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Select City"
                                        name="./pcselectedcity">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="/apps/vida"/>
                                    </pcselectedcity>
                                    <pccomparetext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Compare Link Text"
                                        name="./pccomparetext"/>
                                    <pricetag
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Price tag label"
                                        name="./pricetag"/>
                                    <pcrange
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Range"
                                        name="./pcrange"/>
                                    <certifiedRange
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Certified Range"
                                        name="./certifiedRange"/>
                                    <enableCertifiedRange
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        name="./enableCertifiedRange"
                                        text="Enable Certified Range?"
                                        uncheckedValue="false"
                                        value="{Boolean}true"/>
                                    <pctopspeed
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Top Speed"
                                        name="./pctopspeed"/>
                                    <pcchargingtime
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Charging Time"
                                        name="./pcchargingtime"/>
                                    <pcaccelerator
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Accelerator"
                                        name="./pcaccelerator"/>
                                    <pcridemodes
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Ride Modes"
                                        name="./pcridemodes"/>
                                    <pcseattype
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Seating"
                                        name="./pcseattype"/>
									<tooltiptext
										jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
										fieldLabel="Tooltip Text"
										name="./tooltiptext"/>
                                    <firstproductsku
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="First Product SKU"
                                        name="./firstproductsku"/>
                                    <firstvariantsku
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="First Variant SKU"
                                        name="./firstvariantsku"/>
                                    <firsthoverimg
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="First Hover Image path"
                                        name="./firsthoverimg"
                                        rootPath="/content/dam/vida"/>
                                    <firstisvideo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldLabel="Is Video for First product hover?"
                                        name="./firstisvideo"
                                        text="Is Video for First product hover?"
                                        uncheckedValue="false"
                                        value="{Boolean}true"/>
                                    <firstbaseimg
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="First Base Image path"
                                        name="./firstbaseimg"
                                        rootPath="/content/dam/vida"/>
                                    <firstimgalttext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="First Base Image Alt Text"
                                        name="./firstimgalttext"/>
                                    <secondproductsku
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Second Product SKU"
                                        name="./secondproductsku"/>
                                    <secondvariantsku
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Second Variant SKU"
                                        name="./secondvariantsku"/>
                                    <secondhoverimg
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Second Hover Image path"
                                        name="./secondhoverimg"
                                        rootPath="/content/dam/vida"/>
                                    <secondisvideo
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                        fieldLabel="Is Video for Second product?"
                                        name="./secondisvideo"
                                        text="Is Video for Second product hover?"
                                        uncheckedValue="false"
                                        value="{Boolean}true"/>
                                    <secondbaseimg
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Second Base Image path"
                                        name="./secondbaseimg"
                                        rootPath="/content/dam/vida"/>
                                    <secondimgalttext
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Second Base Image Alt Text"
                                        name="./secondimgalttext"/>
                                    <pcbookdrivebtn
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Book Test Drive CTA Label"
                                        name="./pcbookdrivebtn"/>
                                    <downloadlabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Download Brochure Label"
                                        name="./downloadlabel"/>
                                    <downloadpdf
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                        fieldLabel="Download Brochure PDF path"
                                        name="./downloadpdf"
                                        rootPath="/content/dam/vida"/>
                                    <pcconfigurebtn
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Configure CTA Label"
                                        name="./pcconfigurebtn"/>
                                </items>
                            </column>
                        </items>
                    </general>
                    <priceview
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Pricing View Fields"
                        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                        <items jcr:primaryType="nt:unstructured">
                            <column
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                <items jcr:primaryType="nt:unstructured">
                                    <pricebreakuplabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Pricing Breakup CTA Label"
                                        name="./pricebreakuplabel"/>
                                    <pricingheadline
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Pricing Headline"
                                        name="./pricingheadline"/>
                                    <effectivelabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Effective Price"
                                        name="./effectivelabel"/>
                                    <effectivedesc
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldDescription="To enter text in new line please add &lt;pcnxt> in this field"
                                        fieldLabel="Effective Price Description"
                                        name="./effectivedesc"/>
                                    <exshowroomlabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Ex Showroom Price"
                                        name="./exshowroomlabel"/>
                                    <exshowroomdesc
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldDescription="To enter text in new line please add &lt;pcnxt> in this field"
                                        fieldLabel="Ex Showroom Price Description"
                                        name="./exshowroomdesc"/>
                                    <portablechargerlabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Portable Charger"
                                        name="./portablechargerlabel"/>
                                    <fameincentivelabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="FAME II Incentive"
                                        name="./fameincentivelabel"/>
                                    <fameincentivedesc
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldDescription="To enter text in new line please add &lt;pcnxt> in this field"
                                        fieldLabel="FAME II Incentive Description"
                                        name="./fameincentivedesc"/>
                                    <statesubsidylabel
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="State Subsidy"
                                        name="./statesubsidylabel"/>
                                    <statesubsidydesc
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldDescription="To enter text in new line please add &lt;pcnxt> in this field"
                                        fieldLabel="State Subsidy Description"
                                        name="./statesubsidydesc"/>
                                    <taxdesc
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                        fieldDescription="To enter text in new line please add &lt;pcnxt> in this field"
                                        fieldLabel="Exclusion of Tax Description"
                                        name="./taxdesc"/>
                                    <viewbreakupbtn
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="View Breakup CTA Label"
                                        name="./viewbreakupbtn"/>
                                    <hidebreakupbtn
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Hide Breakup CTA Label"
                                        name="./hidebreakupbtn"/>
                                    <potentialnotify
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="This field is only for mobile view"
                                        fieldLabel="Potential Notification"
                                        name="./potentialnotify"/>
                                    <defaultcity
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                        fieldLabel="Select Default City"
                                        name="./defaultcity">
                                        <datasource
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="/apps/vida"/>
                                    </defaultcity>
                                    <defaultfirstvariantsku
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Default First Variant Sku"
                                        name="./defaultfirstvariantsku"/>
                                    <defaultsecondvariantsku
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldLabel="Default Second Variant Sku"
                                        name="./defaultsecondvariantsku"/>
                                </items>
                            </column>
                        </items>
                    </priceview>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
