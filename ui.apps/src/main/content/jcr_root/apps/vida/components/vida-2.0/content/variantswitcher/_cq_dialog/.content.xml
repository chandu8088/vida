<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Variant Switcher V2.0"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <variantswitcher
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Variant Switcher"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">											
                                            <preTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Pre Title"
                                                name="./preTitle"/>
                                            <preTitleTag
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                emptyText="select tag for Pre Title"
                                                fieldDescription="Please Select the tag for Pre Title"
                                                fieldLabel="Pre Title Tag"
                                                name="./preTitleTag">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <default
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Select Tag"
                                                        value="\0"/>
                                                    <h1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H1"
                                                        value="h1"/>
                                                    <h2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H2"
                                                        value="h2"/>
                                                    <h3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H3"
                                                        value="h3"/>
                                                    <h4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H4"
                                                        value="h4"/>
                                                    <h5
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H5"
                                                        value="h5"/>
                                                    <h6
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H6"
                                                        value="h6"/>
                                                </items>
                                            </preTitleTag>
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title"
                                                name="./title"/>
                                            <enableNewVariantSwitcher
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                checked="{Boolean}false"
                                                name="./enableNewVariantSwitcher"
                                                text="Enable New Variant Switcher"
                                                fieldDescription="If checked New Variant swithcer will be Enabled"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true" />
                                            <titleTag
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                emptyText="select tag for Title"
                                                fieldDescription="Please Select the tag for Title"
                                                fieldLabel="Title Tag"
                                                name="./titleTag">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <default
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Select Tag"
                                                        value="\0"/>
                                                    <h1
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H1"
                                                        value="h1"/>
                                                    <h2
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H2"
                                                        value="h2"/>
                                                    <h3
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H3"
                                                        value="h3"/>
                                                    <h4
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H4"
                                                        value="h4"/>
                                                    <h5
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H5"
                                                        value="h5"/>
                                                    <h6
                                                        jcr:primaryType="nt:unstructured"
                                                        text="H6"
                                                        value="h6"/>
                                                </items>
                                            </titleTag>
                                            <defaultCity
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Default City"
                                                name="./defaultCity"/>
                                            <buttonLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Button Label"
                                                name="./buttonLabel"/>
                                            <buttonLabelLink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Button Label Link"
                                                name="./buttonLabelLink"
                                                rootPath="/content"/>
                                            <buttonNewtab
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                checked="{Boolean}false"
                                                name="./buttonNewtab"
                                                text="Open link in new tab"
                                                fieldDescription="If checked the link will be opened in a new browser tab."
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true" />
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </variantswitcher>
                    <productDetails
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Product Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">											
                                            <productInfo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                composite="{Boolean}true">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                                    name="./productInfo">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <productSku
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Product SKU"
                                                            name="./productSku"/>
                                                        <baseImg
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldLabel="Base Image For SKU"
                                                            name="./baseImg"
                                                            rootPath="/content/dam"/>
                                                        <imageAltBase
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Please provide alternative text to display if image is not available"
                                                            fieldLabel="Alternative text for Base Image"
                                                            name="./imageAltBase"/>
                                                        <imageTitleBase
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Please provide the title text for image"
                                                            fieldLabel="Base Image Title"
                                                            name="./imageTitleBase"/>
                                                        <batteryImg
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldLabel="Battery Image For SKU"
                                                            name="./batteryImg"
                                                            rootPath="/content/dam"/>
                                                        <imageAltBattery
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Please provide alternative text to display if image is not available"
                                                            fieldLabel="Alternative text for Battery Image"
                                                            name="./imageAltBattery"/>
                                                        <imageTitleBattery
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldDescription="Please provide the title text for image"
                                                            fieldLabel="Battery Image Title"
                                                            name="./imageTitleBattery"/>
                                                        <defaultCity
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                            fieldLabel="Default City"
                                                            fieldDescription="Please choose a city to retrieve price details. The default city is set to NEW DELHI~DELHI~INDIA"
                                                            defaultValue="NEW DELHI~DELHI~INDIA"
                                                            name="./defaultCity">
                                                            <datasource
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="/apps/vida" />
                                                        </defaultCity>
                                                        <label
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                            fieldLabel="Label For SKU"
                                                            name="./label"/>
                                                        <labelLink
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                            fieldLabel="Label Link For SKU"
                                                            name="./labelLink"
                                                            rootPath="/content"/>
                                                        <labelNewtab
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                            checked="{Boolean}false"
                                                            name="./labelNewtab"
                                                            text="Open link in new tab"
                                                            fieldDescription="If checked the link will be opened in a new browser tab."
                                                            uncheckedValue="{Boolean}false"
                                                            value="{Boolean}true" />
                                                    </items>
                                                </field>
                                            </productInfo>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </productDetails>
                    <productTitleInfo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Product Title Info"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">											
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <preRemovableBatteryTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldDescription="This will appear positioned above the title for the removable battery(eg: Powered By)."
                                                        fieldLabel="Pre Removable Battery Title"
                                                        name="./preRemovableBatteryTitle"/>
                                                    <batteryQuantity
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Battery Quantity"
                                                        name="./batteryQuantity"/>
                                                    <removableBattery
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Removable Battery"
                                                        name="./removableBattery"/>
                                                    <removableBatteryTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Removable Battery Title"
                                                        name="./removableBatteryTitle"/>
                                                    <chargingTimeTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Charging Time Title"
                                                        name="./chargingTimeTitle"/>
                                                    <acceleratorTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Accelerator Title"
                                                        name="./acceleratorTitle"/>
                                                    <chargingTimeDesktopIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Charging Time Desktop Icon"
                                                        name="./chargingTimeDesktopIcon"
                                                        rootPath="/content/dam"/>
                                                    <enableFastChargingTime
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        name="./enableFastChargingTime"
                                                        text="Enable Fast Charging Time?"
                                                        uncheckedValue="false"
                                                        value="{Boolean}true"/>
                                                    <batteryDesktopIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Battery Desktop Icon"
                                                        name="./batteryDesktopIcon"
                                                        rootPath="/content/dam"/>
                                                    <batteryMobileIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Battery Mobile Icon"
                                                        name="./batteryMobileIcon"
                                                        rootPath="/content/dam"/>   
                                                    <chargingTimeMobileIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Charging Time Mobile Icon"
                                                        name="./chargingTimeMobileIcon"
                                                        rootPath="/content/dam"/>
                                                    <rangeTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Range Title"
                                                        name="./rangeTitle"/>
                                                    <rangeDesktopIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Range Desktop Icon"
                                                        name="./rangeDesktopIcon"
                                                        rootPath="/content/dam"/>
                                                    <rangeMobileIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Range Mobile Icon"
                                                        name="./rangeMobileIcon"
                                                        rootPath="/content/dam"/>
                                                    <certifiedRange
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Certified Range"
                                                        name="./certifiedRange"/>
                                                    <enableCertifiedRange
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                        name="./enableCertifiedRange"
                                                        text="Enable Certified Range?"
                                                        uncheckedValue="false"
                                                        value="{Boolean}true"/>   
                                                    <batteryQuantity
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Battery Quantity"
                                                        name="./batteryQuantity"/> 
                                                    <topSpeedTitle
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Top Speed Title"
                                                        name="./topSpeedTitle"/>
                                                    <topSpeedDesktopIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Top Speed Desktop Icon"
                                                        name="./topSpeedDesktopIcon"
                                                        rootPath="/content/dam"/>
                                                    <topSpeedMobileIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Top Speed Mobile Icon"
                                                        name="./topSpeedMobileIcon"
                                                        rootPath="/content/dam"/>
                                                    <exShowroomLabel
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                        fieldLabel="Ex-Showroom Label"
                                                        name="./exShowroomLabel"/>
                                                    <acceleratorDesktopIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Accelerator Desktop Icon"
                                                        name="./acceleratorDesktopIcon"
                                                        rootPath="/content/dam"/>
                                                    <acceleratorMobileIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Accelerator Mobile Icon"
                                                        name="./acceleratorMobileIcon"
                                                        rootPath="/content/dam"/>
                                                    <batteryDesktopIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Battery Desktop Icon"
                                                        name="./batteryDesktopIcon"
                                                        rootPath="/content/dam"/>
                                                    <batteryMobileIcon
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Battery Mobile Icon"
                                                        name="./batteryMobileIcon"
                                                        rootPath="/content/dam"/>
                                                    <learnMoreRedirection
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        composite="{Boolean}true"
                                                        fieldLabel="Learn More Redirection">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            name="./learnMoreRedirectionList">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <label
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Learn More Redirection Name"
                                                                    name="./name"/>
                                                                <labelLink
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldLabel="Learn More Redirection URL"
                                                                    name="./url"
                                                                    rootPath="/content/vida"/>
                                                            </items>
                                                        </field>
                                                    </learnMoreRedirection>
                                                </items>
                                            </well>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </productTitleInfo>
                    <dataLayer
                        jcr:primaryType="nt:unstructured"
                        jcr:title="DataLayer"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <dataPosition
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="This field specifies the value of linkPosition in the data layer."
                                                fieldLabel="Position"
                                                name="./dataPosition"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </dataLayer>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
