<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Login"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <login
                        jcr:primaryType="nt:unstructured"
                        jcr:title="LOGIN"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <loginlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Login Label"
                                                name="./loginlabel"
                                                value="Login"/>
                                            <img
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                rootPath="/content/dam/vida"
                                                fieldLabel="Home Page Image"
                                                name="./homepageImg"/>
                                            <phone
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone Number Label"
                                                name="./phonelogin"
                                                value="Mobile Number"/>
                                            <pnplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mobile Number Placeholder"
                                                name="./loginplchld"
                                                value="Enter Mobile Number"/>
                                            <accountLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Account Label"
                                                name="./accountLabel"
                                                value="Dont have account?"/>
                                            <registerNow
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Register Now Label"
                                                name="./registerNow"
                                                value="Register Now"/>
                                            <generateOtpBtnLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Generate OTP Label"
                                                name="./generateOtpBtnLabelLogin"
                                                value="Generate OTP"/>
                                            <redirectionUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                rootPath="/content/vida"
                                                fieldLabel="Redirection Url"
                                                fieldDescription="Redirection Url after OTP verification."
                                                name="./redirectionUrl"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </login>
                    <register
                        jcr:primaryType="nt:unstructured"
                        jcr:title="REGISTER"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <registerlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Register Label"
                                                name="./registerlabel"
                                                value="Register"/>
                                            <firstname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="First Name Label"
                                                name="./firstname"
                                                value="First Name"/>
                                            <fnplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="First Name Placeholder"
                                                name="./fnplchld"
                                                value="Enter First Name"/>
                                            <lastname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Last Name Label"
                                                name="./lastname"
                                                value="Last Name"/>
                                            <lnplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Last Name Placeholder"
                                                name="./lnplchld"
                                                value="Enter Last Name"/>
                                            <phone
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone Number Label"
                                                name="./phoneregister"
                                                value="Mobile Number"/>
                                            <phplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone Number Placeholder"
                                                name="./phplchld"
                                                value="Enter Mobile Number"/>
                                            <email
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email Label"
                                                name="./email"
                                                value="Email ID"/>
                                            <emailplaceholder
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email Placeholder"
                                                name="./emailplaceholder"
                                                value="Enter Email"/>
                                            <emailinfolabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email Info Label"
                                                name="./emailinfolabel"/>
                                            <statefieldlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="State Label"
                                                name="./statefieldlabel"
                                                value="State"/>
                                            <statefieldph
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="State Placeholder"
                                                name="./statefieldph"
                                                value="Enter your state"/>
                                            <cityfieldlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="City Label"
                                                name="./cityfieldlabel"
                                                value="City"/>
                                            <cityfieldph
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="City Placeholder"
                                                name="./cityfieldph"
                                                value="Enter your city"/>
                                            <loginid
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Login TnC ID(for experience fragment)"
                                                name="./loginid"/>
                                            <iagreelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="I agree Label"
                                                name="./agreelabel"
                                                value="I agree to"/>
                                            <termsAndCondition
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Terms and conditions Label"
                                                name="./termsAndCondition"
                                                value="Terms and Conditions"/>
                                            <agreebtnlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Agree Button Label"
                                                name="./agreebtnlabel"
                                                value="Agree"/>
                                            <closebtnlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Close Button Label"
                                                name="./closebtnlabel"
                                                value="Close"/>
                                            <notificationLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="WhatsApp Notification Label"
                                                name="./notificationLabel"
                                                value="Receive notifications on Whatsapp"/>
                                            <notificationMsz
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="WhatsApp Notification Message"
                                                name="./notificationMsz"
                                                value="Don’t worry we won’t spam you with any offers, promotions or messages."/>
                                            <existingAccountLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Existing Account Label"
                                                name="./existingAccountLabel"
                                                value="Already have account?"/>
                                            <loginNow
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Login now Label"
                                                name="./loginNowLabel"
                                                value="Login Now"/>                                            
                                            <generateOTP
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Generate the OTP Label"
                                                name="./generateOtpLabelReg"
                                                value="Generate OTP"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </register>
                    <otp
                        jcr:primaryType="nt:unstructured"
                        jcr:title="OTP"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <title
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Title"
                                                name="./title"
                                                value="Verify OTP"/>
                                            <mobileTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Mobile Number Title"
                                                name="./mobileTitle"
                                                value="Verify Mobile Number"/>
                                            <emailTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email Title"
                                                name="./emailTitle"
                                                value="Verify Email"/>
                                            <message
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Message"
                                                name="./message"
                                                value="We've send verification code to"/>
                                            <changeNumberLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Change Number/Email Label"
                                                name="./changeNumberLabel"
                                                value="Change Number"/>
                                            <otpLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="OTP Label"
                                                name="./otpLabel"
                                                value="OTP"/>
                                            <resendLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Resend Label"
                                                name="./resendLabel"
                                                value="Resend"/>
                                            <timerminutes
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Timer Minutes"
                                                name="./timerminutes"
                                                value="1"/>
                                            <timersec
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Timer Seconds"
                                                name="./timersec"
                                                value="59"/>
                                            <verifyBtn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Verify Button Label"
                                                name="./verifyBtn"
                                                value="Verify"/>
                                            <sendOtpBtn
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Send OTP Button Label"
                                                name="./sendOtpBtn"
                                                value="Send Verification Code"/>
                                            <cancelEmailLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Cancel Email Verify Label"
                                                name="./cancelEmailLabel"
                                                value="I’ll do it later"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </otp>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
