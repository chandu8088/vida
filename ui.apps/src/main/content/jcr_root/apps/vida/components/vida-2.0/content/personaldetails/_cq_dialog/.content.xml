<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Personal Details V2.0"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <fieldlabels
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Profile Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <userProfileUploadText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Upload Photo Text"
                                                name="./userProfileUploadText"
                                                value="Lets complete your profile! Upload your photo."/>
                                            <firstnamelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="First name label"
                                                name="./firstnamelabel"
                                                value="First Name"/>
                                            <lastnamelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Last name label"
                                                name="./lastnamelabel"
                                                value="Last Name"/>
                                            <phonenumberlabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone number label"
                                                name="./phonenumberlabel"
                                                value="Phone Number"/>
                                            <emaillabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email label"
                                                name="./emaillabel"
                                                value="Email"/>
                                            <emailinfolabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email Info Label"
                                                name="./emailinfolabel"/>
                                            <pincodelabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Pin code label"
                                                name="./pincodelabel"
                                                value="Pin Code"/>
                                            <cancelctalabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Cancel cta label"
                                                name="./cancelctalabel"
                                                value="Cancel"/>
                                            <saveCtaLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Save CTA Label"
                                                name="./saveCtaLabel"
                                                value="Save Changes"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </fieldlabels>
                    <placeholders
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Place Holders"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <firstname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="First name Place Holder"
                                                name="./firstnameph"
                                                value="Enter First Name"/>
                                            <lastname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Last Name Place Holder"
                                                name="./lastnameph"
                                                value="Enter Last Name"/>
                                            <phonenumberph
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Phone Number Place Holder"
                                                name="./phonenumberph"
                                                value="Enter Phone Number"/>
                                            <emailph
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Email Place Holder"
                                                name="./emailph"
                                                value="Enter Email"/>
                                            <pincode
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Pincode Place Holder"
                                                name="./pincodeph"
                                                value="Enter Pincode"/>
                                            <stateLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="State Label"
                                                name="./stateLabel"
                                                value="Enter State Label"/>
                                            <statePlaceholderText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="State Placeholder Textr"
                                                name="./statePlaceholderText"
                                                value="Enter State Placeholder Text"/>
                                            <cityLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="City Label"
                                                name="./cityLabel"
                                                value="Enter City Label"/>
                                            <cityPlaceholderText
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="City Placeholder Text"
                                                name="./cityPlaceholderText"
                                                value="Enter City Placeholder Text"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </placeholders>
                    <testRideCardConfig
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Test Ride Card Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <testRideHeader
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Test Ride Header"
                                                name="./testRideHeader"/>
                                            <testRideDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Test Ride Description"
                                                name="./testRideDescription"/>
                                            <testRideScooterImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Test Ride Scooter Image"
                                                name="./testRideScooterImage"
                                                rootPath="/content/dam"/>
                                            <testRideScooterImgAlt
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please provide alternative text to display if image is not available"
                                                fieldLabel="Alternative text for image"
                                                name="./testRideScooterImgAlt"/>
                                            <testRideScooterImgTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please provide the title text for image"
                                                fieldLabel="Image Title"
                                                name="./testRideScooterImgTitle"/>
                                            <testRideNavLink
                                                jcr:primaryType="nt:unstructured"
                                                fieldLabel="Test Ride Navigation Link"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                name="./testRideNavLink"
                                                rootPath="/content"/>
                                            <isTestRideUrl
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                name="./isTestRideUrl"
                                                text="Check if this is a test ride url?"
                                                uncheckedValue="{Boolean}false"
                                                value="{Boolean}true" />
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </testRideCardConfig>
                    <buyNowCardConfig
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Buy Now Card Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <buyNowCardHeader
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Buy Now Header"
                                                name="./buyNowCardHeader"/>
                                            <buyNowCardDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Buy Now Description"
                                                name="./buyNowCardDescription"/>
                                            <buyNowCardScooterImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Buy Now Scooter Image"
                                                name="./buyNowCardScooterImage"
                                                rootPath="/content/dam"/>
                                            <buyNowScooterImgAlt
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please provide alternative text to display if image is not available"
                                                fieldLabel="Alternative text for image"
                                                name="./buyNowScooterImgAlt"/>
                                            <buyNowScooterImgTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please provide the title text for image"
                                                fieldLabel="Image Title"
                                                name="./buyNowScooterImgTitle"/>
                                            <buyNowNavLink
                                                jcr:primaryType="nt:unstructured"
                                                fieldLabel="Buy Now Navigation Link"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                name="./buyNowNavLink"
                                                rootPath="/content"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </buyNowCardConfig>
                    <orderCardConfig
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Order Card Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <orderCardHeader
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Order Card Header"
                                                name="./orderCardHeader"/>
                                            <orderCardDescription
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Order Card Description"
                                                name="./orderCardDescription"/>
                                            <orderCardScooterImage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Order Card Scooter Image"
                                                name="./orderCardScooterImage"
                                                rootPath="/content/dam"/>
                                            <orderCardScooterImgAlt
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please provide alternative text to display if image is not available"
                                                fieldLabel="Alternative text for image"
                                                name="./orderCardScooterImgAlt"/>
                                            <orderCardScooterImgTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Please provide the title text for image"
                                                fieldLabel="Image Title"
                                                name="./orderCardScooterImgTitle"/>    
                                            <orderNavLink
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Order Card Navigation Link"
                                                name="./orderNavLink"
                                                rootPath="/content"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </orderCardConfig>
                    <dataLayer
                        jcr:primaryType="nt:unstructured"
                        jcr:title="DataLayer"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <dataPosition
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="This field specifies the value of linkPosition in the data layer."
                                                fieldLabel="Position"
                                                name="./dataPosition"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </dataLayer>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
