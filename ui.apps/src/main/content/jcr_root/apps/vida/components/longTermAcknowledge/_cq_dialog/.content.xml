<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Long Term Test Ride Acknowledgement"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <testRideAcknowledge
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Long Term Test Ride Acknowledge"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                           <longTermAcknowledgeBgImg
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Long Term Test Ride Acknowledge Background Image"
                                                name="./longTermAcknowledgeBgImg"
                                                rootPath="/content/dam/vida"/>
                                            <longTermSuccessTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Long Term Test Ride Success Title"
                                                name="./longTermSuccessTitle"/>
                                            <longTermErrorTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Long Term Test Ride Error Title"
                                                name="./longTermErrorTitle"/>
                                            <longTermSuccessMsg
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Long Term Test Ride Success Message"
                                                name="./longTermSuccessMsg"/>
                                            <longTermErrorMsg
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Long Term Test Ride Error Message"
                                                name="./longTermErrorMsg"/>
                                            <profileBtnLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Profile CTA Label"
                                                name="./profileBtnLabel"/>
                                            <testRideBtnLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Test Ride CTA Label"
                                                name="./testRideBtnLabel"/>
                                            <longTermAcknowledgeInfo
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textarea"
                                                fieldLabel="Long Term Test Ride Acknowledge Information"
                                                name="./longTermAcknowledgeInfo"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </testRideAcknowledge>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
