<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Test Ride Selector"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <testdriveselector
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Test Drive Selector"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                           <testSelectorBgImg
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Test Ride Selector Background Image"
                                                name="./testSelectorBgImg"
                                                rootPath="/content/dam/vida"/>
                                            <testSelectorTitle
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Test Drive Selector Title"
                                                name="./testSelectorTitle"
                                                value="What type of Test Drive"/>
                                            <shortTermLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Short Term Label"
                                                name="./shortTermLabel"
                                                value="Short Term"/>
                                            <shortTermDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Short Term Description"
                                                name="./shortTermDesc"
                                                value="Visit us or request for our expert to come to you"/>
                                            <longTermLabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Long Term Label"
                                                name="./longTermLabel"
                                                value="Long Term"/>
                                            <longTermDesc
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Long Term Description"
                                                name="./longTermDesc"
                                                value="Not satisfied with the restricted timeline, RENT Vida for upto 7 days"/>
                                            <rateMessage
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Rate Message"
                                                name="./rateMessage"/>
                                            <changeloclabel
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Change Location label"
                                                name="./changeloclabel"
                                                required="{Boolean}true"
                                                value="Select different state/city"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </testdriveselector>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
