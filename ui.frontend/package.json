{
  "name": "aem-maven-archetype",
  "version": "1.0.0",
  "description": "Generates an AEM Frontend project with Webpack",
  "repository": {
    "type": "git",
    "url": ""
  },
  "private": true,
  "main": "src/main/webpack/site/main.ts",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "check": "node --version && npm -v",
    "dev": "npm run lint-js-css && cross-env NODE_ENV=development webpack -d --env dev --config ./webpack.dev.js && clientlib --verbose",
    "prod": "npm run lint-js-css && cross-env NODE_ENV=production webpack -p --config ./webpack.prod.js && clientlib --verbose",
    "start": "npm run lint-js-css-fix && cross-env NODE_ENV=development webpack-dev-server --open --config ./webpack.dev.js",
    "sync": "aemsync -d -p ../ui.apps/src/main/content",
    "watch": "webpack-dev-server --config ./webpack.dev.js --env.writeToDisk & watch 'clientlib' ./dist & aemsync -w ../ui.apps/src/main/content",
    "lint": "eslint \"./src/**/*.{js,jsx}\"",
    "lint-fix": "eslint --fix \"./src/**/*.{js,jsx}\"",
    "lint-css": "stylelint \"./src/**/*.{css,scss}\"",
    "lint-css-fix": "stylelint \"./src/**/*.{css,scss}\" --fix",
    "lint-js-css": "npm run lint && npm run lint-css",
    "lint-js-css-fix": "npm run lint-fix && npm run lint-css-fix"
  },
